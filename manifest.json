{
  "manifest_version": 2,

  "name": "lastfm-delete-track-button",
  "version": "1.0",
  "description": "Says hello to Google",
  "content_scripts": [
    {
      "matches": [
        "*://*.last.fm/user/*/library",
        "*://*.last.fm/user/*/library/music/*/*/*"],
      "js": ["delete-code.js"],
      "css": ["delete-styles.css"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html"
  }
}